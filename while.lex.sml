| 93 => (c := !c + 1 ; Tokens.LPAREN(!pos,!c))
| 95 => (c := !c + 1 ;Tokens.RPAREN(!pos,!c))
| 97 => (c := !c + 1 ;Tokens.LBRACE(!pos,!c))
| 99 => (c := !c + 1 ;Tokens.RBRACE(!pos,!c))
